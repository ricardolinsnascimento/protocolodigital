      function gravaPontos(avaliacao, tabela, item, valor, vale, soma_total, quantidade){
        
        var media ;
        document.getElementById(tabela+'-'+item).value = parseInt(document.getElementById(tabela+'-'+item).value);
        if (isNaN(document.getElementById(tabela+'-'+item).value)){
          document.getElementById(tabela+'-'+item).value = 0;
        }
      
        //Calcula total
        var total = 0;
        var valor = 0;
        var vale = 0;
        var somaTotal = '';
        var mediaTotal = 0;
        for (i=1; i<= quantidade; i++){
          valor = parseFloat(document.getElementById(tabela+'-'+i).value);
          var somaTotal = document.getElementById('s'+tabela+'-'+i).value;
          if (isNaN(valor)){
            valor = 0
          };
          if (somaTotal == 'S'){
            total += valor; 
          }          
        }
        if ( tabela == '1.7.1.6' || tabela == '1.7.3.2.4' || tabela == '1.7.3.2.5' || tabela == '1.7.3.2.6' || tabela == '1.7.4'){
          total = 1;
        }
        //alert(total);
        document.getElementById(tabela).value = total.toFixed(2);      
        for (i=1; i<=quantidade; i++){
          valor = parseFloat(document.getElementById(tabela+'-'+i).value); 
          soma_total = document.getElementById('s'+tabela+'-'+i).value;         
          if (isNaN(valor)){
            valor = 0;
          };
          valor = valor.toString();
          vale = parseFloat(document.getElementById('v'+tabela+'-'+i).innerHTML);
          media = (vale * valor) / total;  
          if (isNaN(media)){
            media = 0;
          };   
          mediaTotal += media;
          //alert(media)
          //console.log("p"+tabela+"-"+i)
          document.getElementById("p"+tabela+"-"+i).innerHTML = media.toFixed(2);           
          if (tabela == '1.7.2.4' || tabela == '1.8.2' || tabela == '2.7.2' || tabela == '3.7.2' || tabela == '4.7.2' || tabela == '5.7.2' || tabela == '6.7.2' || tabela == '7.7.2' ){
            document.getElementById("m"+tabela+"-"+i).innerHTML = (media * 2) .toFixed(2);
          }   
          valor = valor.toString();
          gravaPontosFinal(avaliacao, tabela, i, valor, vale, soma_total, media);
        }
        
        if (tabela == '1.7.2.4' || tabela == '1.8.2' || tabela == '2.7.2' || tabela == '3.7.2' || tabela == '4.7.2' || tabela == '5.7.2' || tabela == '6.7.2' || tabela == '7.7.2'){
          document.getElementById("m"+tabela).innerHTML = (mediaTotal * 2).toFixed(2);
        }
        document.getElementById("p"+tabela).innerHTML = mediaTotal.toFixed(2);
        


     }

